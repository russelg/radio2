{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-react-hooks"
  ],
  "jsRules": {},
  "rules": {
    "function-name": false,
    "interface-name": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "max-classes-per-file": false,
    "object-literal-sort-keys": false,
    "variable-name": false,
    "no-this-assignment": [
      true,
      { "allowed-names": ["^self$"], "allow-destructuring": true }
    ],
    "import-name": false,
    "react-hooks-nesting": {
      "options": { "detect-hooks-from-non-react-namespace": true }
    }
  },
  "rulesDirectory": []
}
